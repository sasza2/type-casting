{
  "extends": "airbnb/base",
  "globals": {
    "document": false,
    "expect": false,
    "test": false,
    "window": false
  },
  "parser": "babel-eslint",
  "rules": {
    "func-names": ["error", "never"],
    "semi": 0,
    "import/prefer-default-export": 0,
    "no-unused-vars": [
      "error",
      {
          "varsIgnorePattern": "Element"
      }
    ]
  }
}